  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.7.3",
    "coveralls": "^3.0.6",
    "diffable-html": "^4.0.0",
    "husky": "^3.0.5",
    "jest": "^25.0.0",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.5",
    "minified-size": "^2.0.3",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "files": [
    "build",
    "dom/package.json",
    "html/package.json"
  ],
  "homepage": "https://github.com/PROJECT_ORG/PROJECT_NAME",
  "keywords": [
    "fluurt",
    "marko",
    "runtime"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PROJECT_ORG/PROJECT_NAME"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js --sourcemap",
    "ci:report": "cat coverage/lcov.info | coveralls",
    "ci:test": "jest --ci --coverage",
    "format": "prettier \"**/*.{json,md,js,ts}\" --write",
    "lint": "tsc --noEmit && tslint -t codeFrame -c tslint.json '**/*.ts'",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "size": "npm run build && minified-size --json ./build/dom.esm.js",
    "test": "jest -o",
    "test:inspect": "node --inspect-brk ./node_modules/.bin/jest --runInBand -o --testTimeout=86400000",
    "test:watch": "jest -o --watch"
  }
}
